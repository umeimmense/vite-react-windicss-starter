{
  "version": "0.1.0",
  "name": "vite-react-windicss-starter",
  "description": "Frontend monorepo templates based on pnpm react, vite, windicss",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --cache --fix .",
    "start:package-template": "pnpm -r --filter ./packages/package-template run start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umeimmense/vite-react-windicss-starter.git"
  },
  "keywords": ["vite", "react", "antd", "axios", "monorepo", "pnpm"],
  "author": "umeimmense",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.2.0",
    "classnames": "^2.3.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tailwindcss": "^3.4.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "standard": "^16.0.4",
    "ts-jest": "^27.1.3",
    "type-fest": "^2.11.2",
    "typescript": "^4.5.5",
    "vite": "2.8.3",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-windicss": "^1.7.0",
    "vite-tsconfig-paths": "^3.4.0",
    "windicss": "^3.4.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": ["eslint --cache --fix"],
    "*.json": ["prettier --parser=json --write"],
    "*.css": ["prettier --parser=css --write"],
    "*.less": ["prettier --parser=less --write"]
  },
  "packageManager": "pnpm@6.30.0",
  "dependencies": {
    "antd": "^4.18.7",
    "autoprefixer": "^10.0.2",
    "eslint-plugin-import": "^2.25.4",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "postcss": "^8.4.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-use": "^17.3.2",
    "use-immer": "^0.6.0",
    "vite-plugin-shared-modules": "^0.2.1"
  }
}
